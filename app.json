{
  "expo": {
    "name": "Nateli",
    "slug": "nateli_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "br.inatel.nateli"
    },
    "android": {
      "package": "br.inatel.nateli"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
